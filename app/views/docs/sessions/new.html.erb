<div class="log-in-container">
  <%= form_for(resource, as: resource_name,  class:"ui form", url: session_path(resource_name)) do |f| %>
    <h2>Doctor Sign in</h2>
    <div class="field">
      <%= f.email_field :email, autofocus: true, placeholder: "Email", autocomplete: "email", class: "input" %>
    </div>

    <div class="field">
      <%= f.password_field :password, autocomplete: "current-password", placeholder: "Password", class: "input" %>
    </div>

    <div class="actions">
      <%= f.submit "Log in", class: "submit" %>
    </div>
  <% end %>

  <%= render "docs/shared/links" %>
</div>
